<script>

function error_change(obj) {	
	var src = obj.getAttribute('src');
	var _fileLenth = src.length;
	var _lastDot = src.lastIndexOf('.');

	var ext = src.substring(_lastDot, _fileLenth);
	var ext = ext.substring(1);

	var file_name = src.substring(0, _lastDot);

	switch(ext) {
		case "png":
			obj.setAttribute('src', file_name + ".PNG");
			break;
		case "PNG":
			obj.setAttribute('src', file_name + ".gif");
			break;
		case "gif":
			obj.setAttribute('src', file_name + ".GIF");
			break;
		case "GIF":
			obj.setAttribute('src', file_name + ".jpg");
			break;
		case "jpg":
			obj.setAttribute('src', file_name + ".JPG");
			break;
		case "JPG":
			obj.setAttribute('src', file_name + ".jpeg");
			break;
		case "jpeg":
			obj.setAttribute('src', file_name + ".JPEG");
			break;
		case "JPEG":
			break;
		default:
			obj.setAttribute('src', file_name + ".png");
	}

}

</script>
